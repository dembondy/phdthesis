@ARTICLE{Harris1989,
  author = {Harris, TJ},
  title = {Assessment of control loop performance},
  journal = {Canadian Journal Of Chemical Engineering},
  year = {1989},
  volume = {67},
  pages = {856-861},
  number = {5},
  abstract = {For processes described by linear transfer functions with additive
	disturbances, the best possible control in the mean square sense
	is realized when a minimum variance controller is implemented. It
	is shown that an estimate of the best possible control can be obtained
	by fitting a univariate time series to process data collected under
	routine control. No `identifiability' constraints need be imposed.
	The use of this technique is demonstrated with pilot plant and production
	data},
  issn = {00084034, 1939019x},
  language = {English},
  publisher = {Canadian Soc. Chemical Engineering}
}

